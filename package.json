  "dependencies": {
    "discord.js": "discordjs/discord.js"
  },
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-config-aqua": "^7.2.4"
  },
  "eslintConfig": {
    "extends": "aqua/node",
